#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/node/node_modules/.store/node-media-server@2.4.9/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/node/node_modules/.store/node-media-server@2.4.9/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"   "$basedir/../.store/node-media-server@2.4.9/node_modules/node-media-server/bin/app.js" "$@"
else
  exec node   "$basedir/../.store/node-media-server@2.4.9/node_modules/node-media-server/bin/app.js" "$@"
fi
